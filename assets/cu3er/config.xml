<?xml version="1.0" encoding="utf-8" ?>
    <cu3er>
	<settings>
            <auto_play>
                <defaults time="2" symbol="circular"/>
                <tweenIn x="395" y="45" width="30" height="30" tint="0xFFFFFF" alpha="0.5"/>
                <tweenOver alpha="1"/>
            </auto_play>
            
            <prev_button>
                <defaults round_corners="5,5,5,5"/>
                <tweenOver time="2" tint="0xFFFFFF" scaleX="1.1" scaleY="1.1"/>
                <tweenOut tint="0x000000" />
            </prev_button>

            <prev_symbol>
                <tweenOver tint="0x000000" />
            </prev_symbol>

            <next_button>
                <defaults round_corners="5,5,5,5"/>
                <tweenOver tint="0xFFFFFF"  scaleX="1.1" scaleY="1.1"/>
                <tweenOut tint="0x000000" />
            </next_button>

            <next_symbol>
                <tweenOver tint="0x000000" />
            </next_symbol>

	</settings>

	<slides>
<slide><url>assets/images/frontpage/image1.png</url></slide>
<transition num="4" direction="right" shader="flat" />
<slide><url>assets/images/frontpage/image2.gif</url></slide>
<transition num="6" slicing="vertical" direction="up" shader="flat" delay="0.05" z_multiplier="4" />
<slide><url>assets/images/frontpage/image3.gif</url></slide>
<transition num="5" slicing="horizontal" direction="left" delay="0.05"/>
<slide><url>assets/images/frontpage/image4.gif</url></slide>
<transition num="5" slicing="horizontal" direction="left" delay="0.05"/>        
    </slides>
</cu3er>
